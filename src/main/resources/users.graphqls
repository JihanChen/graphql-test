schema {
  query: UserQuery
  mutation: UserMutation
}
type UserQuery {
  users(request:XxRequest): User
}
type UserMutation {
  addusers(mobile:String,name:String): User
}

type User {
  id:ID!
  name: String!
  sex: String!
  mobile:String!
}

input XxRequest {
  shopId: Long!
  appVersion: String
}


